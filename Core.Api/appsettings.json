{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "copyToOutput": {
      "includeFiles": [ "nlog.config" ]
    }
  },
  "publishOptions": {
    "include": [
      "wwwroot",
      "web.config",
      "nlog.config"
    ]
  }
}
